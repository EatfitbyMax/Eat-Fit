{
  "cli": {
    "version": ">= 3.0.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development",
      "ios": {
        "resourceClass": "m-medium"
      }
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview",
      "env": {
        "EXPO_PUBLIC_USE_EAS_BUILD": "true"
      },
      "ios": {
        "resourceClass": "m-medium"
      }
    },
    "production": {
      "developmentClient": false,
      "distribution": "store",
      "channel": "production",
      "env": {
        "EXPO_PUBLIC_USE_EAS_BUILD": "true"
      },
      "ios": {
        "resourceClass": "m-medium",
        "autoIncrement": true,
        "buildConfiguration": "Release"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "appleId": "m.pacullmarquie@gmail.com",
        "ascAppId": "6737764978",
        "appleTeamId": "2H8T73P8LN"
      }
    }
  }
}