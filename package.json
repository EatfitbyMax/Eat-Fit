{
  "name": "my-app",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "lint": "expo lint",
    "prebuild-check": "node ./scripts/prebuild-check-ios.js",
    "build:ios:preview": "npm run prebuild-check && npx eas build --platform ios --profile preview --clear-cache",
    "build:ios:production": "npm run prebuild-check && npx eas build --platform ios --profile production",
    "postinstall": "node ./scripts/postinstall.js"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "exclude": [
          "@emailjs/react-native",
          "expo-camera",
          "react-native-health",
          "react-native-health/RNHealth",
          "rn-apple-healthkit"
        ],
        "listUnknownPackages": false
      }
    }
  },
  "dependencies": {
    "@emailjs/react-native": "^5.1.0",
    "@expo/vector-icons": "^14.1.0",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-masked-view/masked-view": "^0.3.2",
    "@react-navigation/bottom-tabs": "^7.3.10",
    "@react-navigation/elements": "^2.3.8",
    "@react-navigation/native": "^7.1.6",
    "@stripe/stripe-react-native": "0.45.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/xml2js": "^0.4.14",
    "bcryptjs": "^3.0.2",
    "dotenv": "^17.2.0",
    "expo": "53.0.20",
    "expo-auth-session": "~6.2.1",
    "expo-blur": "~14.1.5",
    "expo-build-properties": "~0.14.8",
    "expo-camera": "~16.1.11",
    "expo-constants": "~17.1.6",
    "expo-crypto": "^14.1.5",
    "expo-dev-client": "^5.2.4",
    "expo-document-picker": "~13.1.6",
    "expo-error-recovery": "^4.0.1",
    "expo-font": "~13.3.1",
    "expo-haptics": "~14.1.4",
    "expo-image": "~2.4.0",
    "expo-image-picker": "^16.1.4",
    "expo-linear-gradient": "~14.1.5",
    "expo-linking": "~7.1.7",
    "expo-router": "^5.1.4",
    "expo-splash-screen": "~0.30.10",
    "expo-status-bar": "~2.2.3",
    "expo-symbols": "~0.4.5",
    "expo-system-ui": "~5.0.10",
    "expo-updates": "~0.28.17",
    "expo-web-browser": "~14.2.0",
    "metro-react-native-babel-transformer": "^0.77.0",
    "pm2": "^6.0.8",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.5",
    "react-native-gesture-handler": "~2.24.0",
    "rn-apple-healthkit": "^0.8.0",
    "react-native-paper": "^5.14.5",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-svg": "15.11.2",
    "react-native-web": "~0.20.0",
    "react-native-webview": "13.13.5",
    "stripe": "^14.21.0",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/plugin-syntax-typescript": "^7.27.1",
    "@babel/plugin-transform-export-namespace-from": "^7.27.1",
    "@expo/config-plugins": "~10.1.1",
    "@expo/ngrok": "^4.1.3",
    "@types/react": "~19.0.10",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~9.2.0",
    "jest": "^29.2.1",
    "jest-expo": "~53.0.9",
    "npm-force-resolutions": "^0.0.10",
    "typescript": "~5.8.3"
  },
  "resolutions": {
    "@expo/config-plugins": "~10.1.1"
  },
  "overrides": {
    "@expo/config-plugins": "~10.1.1"
  },
  "private": true
}
