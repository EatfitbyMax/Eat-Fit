{
  "expo": {
    "name": "EatFit By Max",
    "slug": "eatfitbymax",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/crown-logo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "icon": "./assets/images/crown-logo.png",
      "supportsTablet": true
    },
    "android": {
      "icon": "./assets/images/crown-logo.png",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/crown-logo.png",
        "backgroundColor": "#1E1E1E"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/crown-logo.png"
    },
    "plugins": [
      "expo-router",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
