{
  "expo": {
    "name": "EatFit By Max",
    "slug": "eatfitbymax",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/crown-logo.png",
    "scheme": "eatfitbymax",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "icon": "./assets/images/crown-logo.png",
      "supportsTablet": true,
      "bundleIdentifier": "com.eatfitbymax.app",
      "buildNumber": "1",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSCameraUsageDescription": "Cette app utilise la caméra pour scanner les codes-barres des aliments.",
        "NSHealthShareUsageDescription": "Cette app accède à vos données de santé pour suivre votre activité physique.",
        "NSHealthUpdateUsageDescription": "Cette app met à jour vos données de santé avec vos séances d'entraînement."
      }
    },
    "android": {
      "icon": "./assets/images/crown-logo.png",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/crown-logo.png",
        "backgroundColor": "#1E1E1E"
      },
      "permissions": [
        "android.permission.CAMERA"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/crown-logo.png"
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "816e280b-c76c-4f13-8d77-1ef1898b8ceb"
      }
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "owner": "eatfit",
    "updates": {
      "url": "https://u.expo.dev/816e280b-c76c-4f13-8d77-1ef1898b8ceb"
    }
  }
}