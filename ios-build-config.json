
{
  "buildConfiguration": {
    "platform": "ios",
    "environment": "production",
    "optimizations": {
      "minifyJS": true,
      "treeShaking": true,
      "bundleSize": "optimized"
    }
  },
  "capabilities": [
    "Camera",
    "PhotoLibrary",
    "Motion",
    "HealthKit"
  ],
  "entitlements": {},
  "infoPlistAdditions": {
    "ITSAppUsesNonExemptEncryption": false
  },
  "buildSettings": {
    "ENABLE_BITCODE": false,
    "SWIFT_VERSION": "5.0"
  }
}
